{
  "manifest_version": 3,
  "name": "LinkedIn Connect All",
  "version": "1.0",
  "description": "Automatically connect with all visible profiles on LinkedIn.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
 
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/mynetwork/grow/"],
      "js": ["content.js"]
    }
  ]
}
